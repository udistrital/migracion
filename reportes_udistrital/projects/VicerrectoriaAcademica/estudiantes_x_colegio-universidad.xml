<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Estudiantes por colegio - Universidad</ReportTitle><ReportDescription>Presenta el total de estudiantes de la Universidad por colegio de procedencia, para cada período académico.</ReportDescription><PageSize>US-Legal</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType></DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT COALESCE(nombreoficial, 'NO REGISTRA') COLEGIO_PROCEDENCIA,
(CASE WHEN naturaleza =  'NO_OFICIAL' THEN  'PRIVADO' WHEN naturaleza = '' THEN 'NO REGISTRA' ELSE naturaleza END) TIPO_COLEGIO,
COALESCE(departamento, 'NO REGISTRA') UBICACION,
asp_ape_ano||'-'||asp_ape_per PERIODO,
count (est_cod) TOTAL
FROM acest
LEFT OUTER JOIN acestadm ON est_cod=ead_cod
LEFT OUTER JOIN acasp ON asp_ape_ano=ead_asp_ano AND asp_ape_per=ead_asp_per AND ead_asp_cred=asp_cred
LEFT OUTER JOIN gecolegio ON asp_cod_colegio=codigocolegio
WHERE 1=1
GROUP BY nombreoficial, naturaleza, DEPARTAMENTO, asp_ape_ano, asp_ape_per
ORDER BY total desc, nombreoficial</SQLRaw></SQL><QueryColumns><QueryColumn><Name>COLEGIO_PROCEDENCIA</Name><TableName></TableName><ColumnName>COALESCE(nombreoficial, 'NO REGISTRA')</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF>70mm</ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>COLEGIO DE PROCEDENCIA</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>TIPO_COLEGIO</Name><TableName></TableName><ColumnName>(CASE WHEN naturaleza =  'NO_OFICIAL' THEN  'PRIVADO' WHEN naturaleza = '' THEN 'NO REGISTRA' ELSE naturaleza END)</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>TIPO COLEGIO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>UBICACION</Name><TableName></TableName><ColumnName>COALESCE(departamento, 'NO REGISTRA')</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>UBICACION</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos>