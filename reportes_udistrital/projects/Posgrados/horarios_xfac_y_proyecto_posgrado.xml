<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Horarios por Facultad y Proyecto curricular Posgrados</ReportTitle><ReportDescription>Listado de horarios por facultad y proyecto curricular</ReportDescription><PageSize>US-Letter</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>8</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType></DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>Select cur_ape_ano ANIO, 
cur_ape_per PERIODO,
dep_cod  COD_FACULTAD,
dep_nombre FACULTAD,
cra_cod,
cra_nombre CARRERA, 
asi_cod CODIGO_SIGNATURA, 
asi_nombre ASIGNATURA, 
dia_nombre DIA,
hor_rango HORA, 
(LPAD(CAST(Cur_Cra_Cod AS TEXT),3,'0')||'-'||Cur_Grupo) Grupo,
Sed_Nombre SEDE, 
Edi_Nombre EDIFICIO,
sal_nombre SALON 
From Accursos 
INNER JOIN acasperi ON cur_ape_ano=ape_ano AND cur_ape_per=ape_per AND ape_estado='A'
Inner Join Achorarios On Hor_Id_Curso=Cur_Id 
Inner Join Accra On Cra_Cod=Cur_Cra_Cod 
INNER JOIN actipcra ON cra_tip_cra=tra_cod
inner join gedep on dep_cod=cra_dep_cod 
Inner Join Gedia On Hor_Dia_Nro=Dia_Cod 
Inner Join Gehora On Hor_Cod=Hor_Hora 
Inner join acasi on asi_cod=cur_asi_cod 
left outer Join Gesalones On Hor_Sal_Id_Espacio=Sal_Id_Espacio 
left outer Join Gesede On Sal_Sed_Id=Sed_Id 
left outer join geedificio on sal_edificio=edi_cod 
Where  Cur_Estado='A'
AND tra_cod_nivel in (2, 3 ,4)
order by dep_nombre,cra_nombre,asi_cod,grupo,Hor_Dia_Nro, Hor_Hora</SQLRaw></SQL><QueryColumns><QueryColumn><Name>ANIO</Name><TableName></TableName><ColumnName>cur_ape_ano</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>PERIODO</Name><TableName></TableName><ColumnName>cur_ape_per</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>COD_FACULTAD</Name><TableName></TableName><ColumnName>dep_cod </ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>FACULTAD</Name><TableName></TableName><ColumnName>dep_nom