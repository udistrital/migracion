<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Inscripción automática por proyectos</ReportTitle><ReportDescription>Permite conocer los proyectos que han realizado el proceso de   inscripción automática por proyectos</ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>.DEFAULT</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>8</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType></DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw> SELECT 
DEP_NOMBRE,
CRA_COD,
CRA_NOMBRE,
(SELECT TO_CHAR(ACE_FEC_INI,'DD/MM/YYYY HH24:MI:SS') FROM ACCALEVENTOS WHERE ACE_ANIO=ape_ano AND ACE_PERIODO=ape_per AND ace_cod_evento = 14 AND ace_cra_cod=cra_cod) INICIO_PREINSCRIPCION,
(SELECT TO_CHAR(ACE_FEC_FIN,'DD/MM/YYYY HH24:MI:SS') FROM ACCALEVENTOS WHERE ACE_ANIO=ape_ano AND ACE_PERIODO=ape_per AND ace_cod_evento = 14 AND ace_cra_cod=cra_cod) FIN_PREINSCRIPCION,
( select count(*) from acinspre where ins_ano=ape_ano and ins_per=ape_per and ins_cra_cod=cra_cod) EST_PREINSCRIPCION,
( select count(*) from acins where ins_ano=ape_ano and ins_per=ape_per and ins_cra_cod=cra_cod) EST_INSCRIPCION,
tra_nivel
FROM ACCALEVENTOS
INNER JOIN accra ON cra_cod=ace_cra_cod
INNER JOIN actipcra ON tra_cod=cra_tip_cra
INNER JOIN gedep ON dep_cod=cra_dep_cod
INNER JOIN acasperi ON ape_ano=ace_anio AND ape_per=ace_periodo
WHERE ape_estado='A'
AND ace_cod_evento=90
AND ace_cra_cod=cra_cod
ORDER BY tra_nivel desc,dep_nombre,cra_cod</SQLRaw></SQL><QueryColumns><QueryColumn><Name>DEP_NOMBRE</Name><TableName></TableName><ColumnName>DEP_NOMBRE</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>FACULTAD</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>CRA_COD</Name><TableName></TableName><ColumnName>CRA_COD</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF>15mm</ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>COD.</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>CRA_NOMBRE</Name><TableName></TableName><ColumnName>CRA_NOMBRE</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF>50mm</ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>PROYECTO CURRICULAR</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>INICIO_PREINSCRIPCION</Name><TableName>