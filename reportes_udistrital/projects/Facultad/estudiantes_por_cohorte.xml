<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Por cohorte</ReportTitle><ReportDescription>Muestra el listado de estudiantes por cohorte</ReportDescription><PageSize>US-Letter</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>8</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType></DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT 
	(CASE WHEN LENGTH( est_cod )=11 THEN substr(est_cod,0,4)||'-'||substr(est_cod,5,1) 
		WHEN LENGTH( est_cod )=7 THEN substr(est_cod,0,2)||'-'||substr(est_cod,3,1) 
		ELSE '' END)	AS COHORTE,
	dep_cod 		AS COD_FACULTAD,
	dep_nombre 		AS FACULTAD,
	cra_cod 		AS COD_PROYECTO,
	cra_nombre 		AS PROYECTO,
	est_cod 		AS CODIGO,
	est_nombre 		AS NOMBRE,
	eot_email 		AS CORREO,
	eot_email_ins 		AS CORREO2,
	est_estado_est  	AS COD_ESTADO,
	estado_descripcion 	AS ESTADO
FROM acest 
INNER JOIN accra ON est_cra_cod=cra_cod
LEFT OUTER JOIN actipcra ON cra_tip_cra=tra_cod
INNER JOIN gedep ON dep_cod=cra_dep_cod
LEFT OUTER JOIN acestotr ON est_cod=eot_cod
INNER JOIN acestado ON est_estado_est=estado_cod 
WHERE  tra_cod_nivel=1--pregrado
AND (CASE WHEN LENGTH( est_cod )=11 THEN substr(est_cod,0,4) 
		WHEN LENGTH( est_cod )=7 THEN substr(est_cod,0,2) 
		ELSE '' END)&gt;=2000
    AND (CASE WHEN LENGTH( est_cod )=11 THEN substr(est_cod,0,4) 
		WHEN LENGTH( est_cod )=7 THEN substr(est_cod,0,2) 
		ELSE '' END)&lt;=to_char(sysdate,'YYYY')
    AND est_cra_cod&lt;&gt;0
ORDER BY dep_nombre, cra_cod,est_cod
</SQLRaw></SQL><QueryColumns><QueryColumn><Name>COHORTE</Name><TableName></TableName><ColumnName>(CASE WHEN LENGTH( est_cod )=11 THEN substr(est_cod,0,4)||'-'||substr(est_cod,5,1) 
