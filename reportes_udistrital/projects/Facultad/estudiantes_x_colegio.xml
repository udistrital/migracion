<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Estudiantes por colegio</ReportTitle><ReportDescription>Presenta el total de estudiantes del Proyecto Curricular por Colegio de Procedencia, para cada período académico.</ReportDescription><PageSize>US-Legal</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType></DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT est_cra_cod CRA,
cra_nombre PROYECTO,
nombreoficial COLEGIO_PROCEDENCIA,
decode(naturaleza, 'NO_OFICIAL', 'PRIVADO','','NO REGISTRA',naturaleza) TIPO_COLEGIO,
NVL(departamento, 'NO REGISTRA') UBICACION,
count (est_cod) TOTAL
FROM acasp
INNER JOIN gecolegio ON asp_cod_colegio=codigocolegio
INNER JOIN acestadm ON asp_ape_ano=ead_asp_ano AND asp_ape_per=ead_asp_per AND ead_asp_cred=asp_cred
INNER JOIN acest ON est_cod=ead_cod
INNER JOIN acestotr ON est_cod=eot_cod
INNER JOIN accra ON cra_cod=est_cra_cod
INNER JOIN acestado ON est_estado_est=estado_cod
WHERE 1=1
GROUP BY est_cra_cod,cra_nombre,nombreoficial,naturaleza,DEPARTAMENTO
ORDER BY est_cra_cod,total desc,nombreoficial</SQLRaw></SQL><QueryColumns><QueryColumn><Name>CRA</Name><TableName></TableName><ColumnName>est_cra_cod</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>COD. PROYECTO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>PROYECTO</Name><TableName></TableName><ColumnName>cra_nombre</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>PROYECTO CURRICULAR</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>COLEGIO_PROCEDENCIA</Name><TableName></TableName><ColumnName>nombreoficial</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF>70mm</ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>COLEGIO DE PROCEDENCIA</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>TIPO_COLE