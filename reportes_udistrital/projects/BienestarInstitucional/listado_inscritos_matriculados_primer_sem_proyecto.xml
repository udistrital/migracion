<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Listado matrículados a primer semestre</ReportTitle><ReportDescription>Muestra el listado de los estudiantes que fueron admitidos y se matricularon.</ReportDescription><PageSize>US-Letter</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>8</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType></DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT rownum,PERIODO,COD_INICIAL,COD_FACULTAD,FACUTAD,COD_CRA,CRA_NOM,PENSUM,DOCU_COORDINADOR,CODIGO,NOMBRE,NRO_IDEN,ACUERDO,CREDITOS,EMAIL,ESTADO,ESTADO_ACTUAL
FROM (
SELECT DISTINCT
substr(A.est_cod,0,4)||'-'||substr(A.est_cod,5,1) AS PERIODO,
substr(A.est_cod,0,5) 	AS COD_INICIAL,
dep_cod           AS COD_FACULTAD,
dep_nombre        AS FACUTAD,
A.est_cra_cod 		AS COD_CRA,
CRA.cra_nombre 		AS CRA_NOM,
A.est_pen_nro   		AS PENSUM,
CRA.cra_emp_nro_iden	AS DOCU_COORDINADOR,
A.est_cod  		AS CODIGO,
A.est_nombre  		AS NOMBRE,
A.est_nro_iden 		AS NRO_IDEN,
decode(A.est_acuerdo,1993027,'027 de 1993',2009007,'007 de 2009',2011004,'004 de 2011',A.est_acuerdo) ACUERDO,
DECODE(A.est_ind_cred,'S','CREDITOS','N','HORAS','') AS CREDITOS,
eot_email		AS EMAIL,
est_estado_est ESTADO,
estado_descripcion ESTADO_ACTUAL
FROM
acest A,accra CRA,acestotr,acestado,gedep
WHERE
A.est_cra_cod=CRA.cra_cod
AND CRA.cra_dep_cod=dep_cod
AND A.est_cod=eot_cod (+)
AND A.est_estado_est=estado_cod
AND A.est_estado_est!='N'
ORDER BY
PERIODO,A.est_cra_cod,A.est_cod)</SQLRaw></SQL><QueryColumns><QueryColumn><Name>PERIODO</Name><TableName></TableName><ColumnName>PERIODO</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF>20mm</ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>PERÍODO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>COD_INICIAL</Name><TableName></TableName><ColumnName>COD_INICIAL</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>COD_CRA</Name><TableName></TableName><ColumnName>COD_CRA</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>COD. PROYECTO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>CRA_NOM</Name><TableName></TableName><ColumnName>CRA_NOM</ColumnNa