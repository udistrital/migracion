<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Estudiantes con niveles de riesgo Fac. Artes</ReportTitle><ReportDescription>Listado de estudiantes por nivel de riesgo de la facultad Artes, de acuerdo al ultimo cierre</ReportDescription><PageSize>US-Letter</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>7</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType></DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT 
	reg_ano		AS ANIO,
	reg_per		AS PERIODO,
	dep_cod		AS COD_FACULTAD,
	dep_nombre	AS FACULTAD,
	cra_cod		AS COD_PROYECTO,
	cra_nombre	AS PROYECTO,
	est_cod		AS CODIGO,
	est_nombre	AS NOMBRE,
 	EOT_FECHA_NAC 	AS FECHA_NCTO,
 	EST_SEXO 	AS GENERO,
 	ASP_TIP_ICFES 	AS TIPO_ICFES,
   	ASP_PTOS 	AS PUNTAJE_ICFES,
   	TI_NOMBRE 	AS TIPO_INSCRIPCION,
   	EST_TELEFONO 	AS TEL_FIJO,
   	EOT_EMAIL_INS 	AS EMAIL_INST,
   	EOT_EMAIL 	AS EMAIL,
	FA_PROMEDIO_NOTA(EST_COD) 		AS PROMEDIO_ACUMULADO,
 	(est_estado_est||'-'||estado_nombre) 	AS ESTADO_ACTUAL,
 	(SELECT count(distinct(NOT_ASI_COD))
		FROM ACNOT
	    	WHERE EST_CRA_COD = NOT_CRA_COD
		AND EST_COD = NOT_EST_COD
		AND NOT_EST_REG = 'A') 		AS TOTAL_ASIG_TOMADAS,
 	(SELECT count(distinct(NOT_ASI_COD))
		FROM ACNOT
		WHERE EST_CRA_COD = NOT_CRA_COD
		AND EST_COD = NOT_EST_COD
		AND NOT_EST_REG = 'A'
		AND NOT_NOTA &lt; FUA_NOTA_APROBATORIA(CRA_COD)) 	AS TOTAL_ASIG_REPROBADAS,
	(reg_rendimiento_ac*100) 		AS PORC_RENDIMIENTO,
	(reg_indice_riesgo*100)			AS PORC_RIESGO,
  	(CASE WHEN (reg_indice_riesgo*100)&gt;=0  AND (reg_indice_riesgo*100)&lt;=5 THEN 'NIVEL BAJO' 
	  	WHEN (reg_indice_riesgo*100)&gt;5  AND (reg_indice_riesgo*100)&lt;=15 THEN 'NIVEL MEDIO BAJO' 
	  	WHEN (reg_indice_riesgo*100)&gt;15  AND (reg_indice_riesgo*100)&lt;=20 THEN 'NIVEL MEDIO' 
	  	WHEN (reg_indice_riesgo*100)&gt;20  AND (reg_indice_riesgo*100)&lt;=30 THEN 'NIVEL MEDIO ALTO' 
	  	WHEN (reg_indice_riesgo*100)&gt;30 THEN 'NIVEL ALTO' 
	  END) 					AS NIVEL_RIESGO,
(select count(*) from MNTAC.V_TOT_MATRI_APE_PER where EST_CRA_COD = MAT_CRA_COD and EST_COD = MAT_EST_COD) NUM_MAT,
(select count(*)
    from REGLAMENTO
    where EST_CRA_COD = REG_CRA_COD
       and EST_COD = REG_EST_COD
       and REG_REGLAMENTO = 'S'
    having(max(REG_ANO)) is not null) NUMERO_PRUEBAS_ACADEMICAS
FROM acest
INNER JOIN reglamento ON reg_est_cod=est_cod AND reg_ano||'-'||reg_per= (SELECT ape_ano||'-'||ape_per FROM acasperi WHERE ape_estado='P')
INNER JOIN accra ON est_cra_cod=cra_cod
INNER JOIN GEDEP ON cra_dep_cod=dep_cod
LEFT OUTER JOIN ACESTOTR ON eot_cod=est_cod
LEFT OUTER JOIN ACESTADM ON EST_COD = EAD_COD
LEFT OUTER JOIN ACASP ON EAD_ASP_ANO = ASP_APE_ANO and EAD_ASP_PER = ASP_APE_PER and EAD_ASP_CRED = ASP_CRED
LEFT OUTER JOIN ACTIPINS ON ASP_TI_COD = TI_COD
INNER JOIN ACESTADO ON est_estado_est=estado_cod
WHERE reg_indice_riesgo&gt;0
AND EST_ESTADO_EST IN ('H','O','L','B','A','T','V','J','Q')
AND dep_cod=101
ORDER BY reg_ano,reg_per,nivel_riesgo,dep_cod,cra_cod,est_cod</SQLRaw></SQL><QueryColumns><QueryColumn><Name>ANIO</Name><TableName></TableName><ColumnName>reg_ano		AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>AÃ‘O CIERRE</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>PERIODO</Name><TableName></TableName><ColumnName>reg_per		AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>PERIODO CIERRE</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>COD_FACULTAD</Name><TableName></TableName><ColumnName>dep_cod		AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>FACULTAD</Name><TableName></TableN