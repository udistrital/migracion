<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Listado de aspirantes</ReportTitle><ReportDescription>Muestra el listado de los aspirantes que se presentaron en el proceso de admisiones </ReportDescription><PageSize>US-Letter</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>8</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType></DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT DISTINCT 
	(ASP.asp_ape_ano||'-'||ASP.asp_ape_per) 	AS PERIODO,
        CRA.cra_dep_cod AS COD_DEP, 
        DEP.dep_nombre AS FACULTAD, 
	ASP.asp_cra_cod 	AS COD_CRA,
	CRA.cra_nombre 		AS CRA_NOM,
	CRA.cra_nombre 		AS CRA_NOM,
	CRA.cra_emp_nro_iden	AS DOCU_COORDINADOR,
	ASP.asp_cred 		AS CREDENCIAL,
	ASP.asp_nro_iden 	AS NRO_IDEN,
	ASP.asp_apellido 	AS APELLIDO,
	ASP.asp_nombre  	AS NOMBRE,
	ASP.asp_email		AS EMAIL,
	INS.ti_nombre 		AS TIPO_INSC,
        ASP_TIP_ICFES           AS TIPO_ICFES,
        ASP_PTOS                AS PUNTAJE_ICFES
FROM
	acasp ASP,
	accra CRA,
	ACTIPINS INS, 
        gedep DEP  

WHERE
 	ASP.asp_cra_cod=CRA.cra_cod
        AND CRA.cra_dep_cod=DEP.dep_cod
	AND ASP.asp_estado='A'
	AND ASP.asp_ti_cod=INS.ti_cod

ORDER BY
	PERIODO,
	ASP.asp_cra_cod,
	ASP.asp_apellido
 </SQLRaw></SQL><QueryColumns><QueryColumn><Name>PERIODO</Name><TableName></TableName><ColumnName>DISTINCT 
