<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Información docentes de planta</ReportTitle><ReportDescription>Listado que muestra información de los docentes</ReportDescription><PageSize>US-Letter</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>8</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType></DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT DEP_COD cod_fac,
   DEP_NOMBRE facultad,
   EMP_NRO_IDEN cedula,
   EMP_NOMBRE nombre,
   emp_desde fecha_ing,
   esa_sueldo sueldo,
   PUN_PUNTAJE puntos,
   (CASE WHEN EMP_REGIMEN ='N' THEN 'NUEVO' WHEN EMP_REGIMEN ='A' THEN 'ANTIGUO' ELSE 'NO REGISTRA' END) regimen,
   car_nombre CARGO
FROM GEDEP, PEEMP, PECARGO, PREMPSAL, prpuntajes
WHERE DEP_COD = EMP_DEP_COD
   AND emp_car_cod = car_cod
   AND car_tc_cod IN ('DP','DH','DC')
   AND EMP_COD = PUN_EMP_COD
   AND pun_estado = 'A'
   AND emp_cod = esa_cod
   AND emp_regimen = 'N'
   AND EMP_ESTADO_E&lt;&gt;'R'
UNION  
SELECT DEP_COD,
   DEP_NOMBRE,
   EMP_NRO_IDEN,
   EMP_NOMBRE,
   emp_desde,
   esa_sueldo,
   0,
   (CASE WHEN EMP_REGIMEN ='N' THEN 'NUEVO' WHEN EMP_REGIMEN ='A' THEN 'ANTIGUO' ELSE 'NO REGISTRA' END) regimen,
   car_nombre
FROM GEDEP, PEEMP, PECARGO, PREMPSAL
WHERE DEP_COD = EMP_DEP_COD
   AND emp_car_cod = car_cod
   AND car_tc_cod IN ('DP','DH','DC')
   AND emp_cod = esa_cod
   AND emp_regimen = 'A'
   AND EMP_ESTADO_E&lt;&gt;'R'
ORDER BY cod_fac,cedula ASC</SQLRaw></SQL><QueryColumns><QueryColumn><Name>cod_fac</Name><TableName></TableName><ColumnName>DEP_COD</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>COD. DEPENDENCIA</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>facultad</Name><TableName></TableName><ColumnName>DEP_NOMBRE</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>DEPENDENCIA</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>cedula</Name><TableName></TableName><ColumnName>EMP_NRO_IDEN</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF>20mm</ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>IDENTIFICACIÓN</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>nombre</Name><TableName></TableName><ColumnName>EM