<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Promedios ponderados</ReportTitle><ReportDescription>Presenta el listado de estudiantes con promedios ponderados ordenados de mayor a menor, del período académico inmediatamente anterior</ReportDescription><PageSize>US-Legal</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType></DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT ROWNUM,COD_PROYECTO,
PROYECTO,
COD_ESTUDIANTE,
NOMBRE_ESTUDIANTE,
ESTADO,
DESCRIPCION,
INSCRITOS,
PROM,
ACUMULADO
FROM (
SELECT est_cra_cod COD_PROYECTO,
cra_nombre PROYECTO,
est_cod COD_ESTUDIANTE,
est_nombre NOMBRE_ESTUDIANTE,
est_estado_est ESTADO,
estado_descripcion DESCRIPCION,
COUNT(not_asi_cod) INSCRITOS,
to_char(fa_promedio_sem(est_cod,ape_ano,ape_per),'9D99') as PROM,
to_char(fa_promedio_nota(est_cod),'9D99') as ACUMULADO
FROM acest
INNER JOIN accra ON cra_cod=est_cra_cod
INNER JOIN acasperi ON 1=1
INNER JOIN acestado ON est_estado_est=estado_cod
INNER JOIN acnot ON not_est_cod=est_cod AND not_ano=ape_ano and not_per=ape_per
WHERE ape_estado='P'
AND est_estado_est in ('A','B','V','J','H','T','O')
GROUP BY est_cra_cod,cra_nombre,est_cod,est_nombre,est_estado_est,estado_descripcion,fa_promedio_sem(est_cod,ape_ano,ape_per),fa_promedio_nota(est_cod)
ORDER BY PROM DESC,ACUMULADO DESC)
</SQLRaw></SQL><QueryColumns><QueryColumn><Name>COD_PROYECTO</Name><TableName></TableName><ColumnName>COD_PROYECTO</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>COD. PROYECTO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>PROYECTO</Name><TableName></TableName><ColumnName>PROYECTO</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>PROYECTO CURRICULAR</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>NOMBRE_ESTUDIANTE</Name><TableName></TableName><ColumnName>NOMBRE_ESTUDIANTE</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>ESTADO</Name><TableName></TableName><ColumnName>ESTADO</ColumnName><Co