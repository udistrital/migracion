<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Horario por estudiante</ReportTitle><ReportDescription>Horario para un estudiante, se debe realizar la búsqueda ingresando el código del estudiante.</ReportDescription><PageSize>US-Letter</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>8</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType></DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT est_cod,
  est_nombre,
  est_nro_iden,
  ins_asi_cod,
  asi_nombre,
  cur_cra_cod||'-'||cur_grupo cod_grupo,
  dia_nombre,
  achorarios.hor_hora,
  sed_nombre,
  edi_nombre,
  gesalones.sal_nombre,
  car_doc_nro column12,
  acdocente.doc_apellido||' '||doc_nombre Docente
FROM acest
INNER JOIN acins
ON ins_est_cod=est_cod
INNER JOIN acasi
ON asi_cod=ins_asi_cod
INNER JOIN accursos
ON cur_id=ins_gr
INNER JOIN achorarios
ON hor_id_curso=cur_id
INNER JOIN gesalones
ON sal_id_espacio=hor_sal_id_espacio
INNER JOIN gedia
ON gedia.dia_cod=achorarios.hor_dia_nro
INNER JOIN geedificio
ON EDI_COD=gesalones.sal_edificio
INNER JOIN gesede
ON sed_cod=gesalones.sal_sed_cod
LEFT OUTER JOIN accargas
ON car_hor_id=hor_id
LEFT OUTER JOIN acdocente
ON doc_nro_iden=car_doc_nro
INNER JOIN acasperi
ON ape_ano      =ins_ano
AND ape_per     =ins_per
WHERE ape_estado='A'
ORDER BY asi_cod,
  dia_nombre,
  hor_hora</SQLRaw></SQL><QueryColumns><QueryColumn><Name>est_cod</Name><TableName></TableName><ColumnName>est_cod</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Código</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>est_nombre</Name><TableName></TableName><ColumnName>est_nombre</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF>30mm</ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Nombre</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>est_nro_iden</Name><TableName></TableName><ColumnName>est_nro_iden</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Documento</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>ins_asi_cod</Name><TableName></TableName><ColumnName>ins_asi_cod</ColumnName><Colum