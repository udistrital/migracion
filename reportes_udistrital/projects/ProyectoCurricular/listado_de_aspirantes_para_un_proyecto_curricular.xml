<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Listado de aspirantes</ReportTitle><ReportDescription>Muestra el listado de los aspirantes que se presentaron en el proceso de admisiones </ReportDescription><PageSize>US-Letter</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>8</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType></DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT row_number() over ()  AS ROWNUM,
PERIODO,
COD_CRA,
CRA_NOM,
DOCU_COORDINADOR,
CREDENCIAL,
NRO_IDEN,
APELLIDO,
NOMBRE,
EMAIL,
TIPO_INSC,
TIPO_ICFES,
PUNTAJE_ICFES,
ESTRATO
FROM (
SELECT DISTINCT 
	(ASP.asp_ape_ano||'-'||ASP.asp_ape_per) 	AS PERIODO,
	ASP.asp_cra_cod 	AS COD_CRA,
	CRA.cra_nombre 		AS CRA_NOM,
	CRA.cra_emp_nro_iden	AS DOCU_COORDINADOR,
	ASP.asp_cred 		AS CREDENCIAL,
	ASP.asp_nro_iden 	AS NRO_IDEN,
	ASP.asp_apellido 	AS APELLIDO,
	ASP.asp_nombre  	AS NOMBRE,
	ASP.asp_email		AS EMAIL,
	INS.ti_nombre 		AS TIPO_INSC,
        ASP_TIP_ICFES           AS TIPO_ICFES,
        ASP_PTOS                AS PUNTAJE_ICFES,
        CASE WHEN ASP_ESTRATO&lt;7 THEN ASP_ESTRATO::TEXT
        ELSE 'NO REGISTRA' END AS ESTRATO
FROM
	acasp ASP,
	accra CRA,
	ACTIPINS INS 

WHERE
 	ASP.asp_cra_cod=CRA.cra_cod
	AND ASP.asp_estado='A'
	AND ASP.asp_ti_cod=INS.ti_cod

ORDER BY
	PERIODO,
	ASP.asp_cra_cod,
	ASP.asp_apellido
) AS A
 </SQLRaw></SQL><QueryColumns><QueryColumn><Name>ROWNUM</Name><TableName></TableName><ColumnName>row_number() over ()  AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>No.</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>PERIODO</Name><TableName></TableName><ColumnName>PERIODO</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF>20mm</ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>PERÍODO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>COD_CRA</Name><TableName></TableName><ColumnName>COD_CRA</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF>20mm</ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>COD. PROYECTO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>CRA_NOM</Name><TableName></TableName><ColumnName>CRA_NOM</ColumnName><ColumnType>c