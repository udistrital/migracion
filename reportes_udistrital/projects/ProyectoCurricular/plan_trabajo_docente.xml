<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Plan de trabajo docente</ReportTitle><ReportDescription>Muestra el listado de las actividades registradas del plan de trabajo de un docente de un período.</ReportDescription><PageSize>US-Letter</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>8</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType></DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT ROWNUM,ANIO, PER, PERIODO, DOC_DOCENTE, NOMBRE_DOCENTE, ACTIVIDAD, DIA_NOMBRE, HORA, EDIFICIO, SEDE, SALON, FECHA_REGISTRO, ESTADO, DIA, COD_HORA, COD_ACTIVIDAD, TIPO_VINCULACION
FROM (SELECT DPT_APE_ANO AS ANIO,
DPT_APE_PER AS PER,
(DPT_APE_ANO||'-'||DPT_APE_PER) AS PERIODO,
DPT_DOC_NRO_IDEN AS DOC_DOCENTE,
(DOC_APELLIDO||' '||DOC_NOMBRE) AS NOMBRE_DOCENTE,
DAC_NOMBRE AS ACTIVIDAD,
DIA_ABREV AS DIA_NOMBRE,
HOR_LARGA AS HORA,
(SED_ID||' - '||edi_nombre) AS EDIFICIO,
TO_CHAR(sed_nombre||' - '||edi_nombre) AS SEDE,
TO_CHAR(sal_id_espacio||' - '||sal_nombre) AS SALON,
DPT_FECHA AS FECHA_REGISTRO,
DPT_ESTADO AS ESTADO,
DIA_COD AS DIA,
HOR_COD AS COD_HORA,
DPT_DAC_COD AS COD_ACTIVIDAD,
(decode(tvi_cod,1,'PL',6,'PL',0,'SD','VE')||'-'||tvi_nombre) AS TIPO_VINCULACION
FROM acdocplantrabajo
INNER JOIN acdocactividad ON dac_cod=dpt_dac_cod AND dac_estado='A'
INNER JOIN gedia ON dia_cod=dpt_dia_nro AND dia_estado='A'
INNER JOIN gehora ON hor_cod=dpt_hora AND hor_estado='A'
INNER JOIN gesede ON sed_cod=dpt_sed_cod AND sed_estado='A'
INNER JOIN gesalones ON dpt_sed_cod=sed_cod AND sal_id_espacio=dpt_sal_cod AND sal_estado='A'
INNER JOIN geedificio ON sal_edificio= edi_cod
INNER JOIN actipvin ON tvi_cod=dpt_tvi_cod
INNER JOIN acdocente ON dpt_doc_nro_iden=doc_nro_iden
UNION
SELECT DPT_APE_ANO AS ANIO,
DPT_APE_PER AS PER,
(DPT_APE_ANO||'-'||DPT_APE_PER) AS PERIODO,
DPT_DOC_NRO_IDEN AS DOC_DOCENTE,
(DOC_APELLIDO||' '||DOC_NOMBRE) AS NOMBRE_DOCENTE,
DAC_NOMBRE AS ACTIVIDAD,
DIA_ABREV AS DIA_NOMBRE,
HOR_LARGA AS HORA,
('') AS EDIFICIO,
TO_CHAR(sed_nombre) AS SEDE,
TO_CHAR(sal_cod||' - '||sal_descrip) AS SALON,
DPT_FECHA AS FECHA_REGISTRO,
DPT_ESTADO AS ESTADO,
DIA_COD AS DIA,
HOR_COD AS COD_HORA,
DPT_DAC_COD AS COD_ACTIVIDAD,
(decode(tvi_cod,1,'PL',6,'PL',0,'SD','VE')||'-'||tvi_nombre) AS TIPO_VINCULACION
FROM acdocplantrabajo
INNER JOIN acdocactividad ON dac_cod=dpt_dac_cod AND dac_estado='A'
INNER JOIN gedia ON dia_cod=dpt_dia_nro AND dia_estado='A'
INNER JOIN gehora ON hor_cod=dpt_hora AND hor_estado='A'
INNER JOIN gesede ON sed_cod=dpt_sed_cod AND sed_estado='A'
INNER JOIN gesalon ON sal_sed_cod=sed_cod AND TO_CHAR(sal_cod)=TO_CHAR(dpt_sal_cod) AND sal_estado='A'
INNER JOIN actipvin ON tvi_cod=dpt_tvi_cod
INNER JOIN acdocente ON dpt_doc_nro_iden=doc_nro_iden
ORDER BY ANIO,PER,DOC_DOCENTE,DIA,COD_HORA)
WHERE ESTADO = 'A'</SQLRaw></SQL><QueryColumns><QueryColumn><Name>ANIO</Name><TableName></TableName><ColumnName>ANIO</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>PER</Name><TableName></TableName><ColumnName>PER</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>PERIODO</Name><TableName></TableName><ColumnName>PERIODO</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF>20mm</ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>PERÍODO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>DOC_DOCENTE</Name><TableName></TableName><ColumnName>DOC_DOCENTE</ColumnName><ColumnType>char</ColumnType><Column