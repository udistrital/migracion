<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Eventos Cierre de Semestre 2014-3</ReportTitle><ReportDescription>Permite conocer los proyectos que han realizado el cierre de semestre</ReportDescription><PageSize>US-Letter</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType></DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT 
DEP_NOMBRE,
CRA_COD,
CRA_NOMBRE,
(SELECT TO_CHAR(ACE_FEC_INI,'DD/MM/YYYY HH24:MI:SS') FROM ACCALEVENTOS WHERE ACE_ANIO=APE_ANO AND ACE_PERIODO=APE_PER AND ace_cod_evento = 60 AND ace_cra_cod=cra_cod) INICIO_CIERRE,
(SELECT TO_CHAR(ACE_FEC_FIN,'DD/MM/YYYY HH24:MI:SS') FROM ACCALEVENTOS WHERE ACE_ANIO=APE_ANO AND ACE_PERIODO=APE_PER AND ace_cod_evento = 60 AND ace_cra_cod=cra_cod) FIN_CIERRE,
(SELECT TO_CHAR(ACE_FEC_INI,'DD/MM/YYYY HH24:MI:SS') FROM ACCALEVENTOS WHERE ACE_ANIO=APE_ANO AND ACE_PERIODO=APE_PER AND ace_cod_evento = 71 AND ace_cra_cod=cra_cod) INICIO_NOTAS_CERRADAS,
(SELECT TO_CHAR(ACE_FEC_FIN,'DD/MM/YYYY HH24:MI:SS') FROM ACCALEVENTOS WHERE ACE_ANIO=APE_ANO AND ACE_PERIODO=APE_PER AND ace_cod_evento = 71 AND ace_cra_cod=cra_cod) FIN_NOTAS_CERRADAS,
(SELECT TO_CHAR(ACE_FEC_INI,'DD/MM/YYYY HH24:MI:SS') FROM ACCALEVENTOS WHERE ACE_ANIO=APE_ANO AND ACE_PERIODO=APE_PER AND ace_cod_evento = 72 AND ace_cra_cod=cra_cod) INICIO_CAMBIO_ESTADOS,
(SELECT TO_CHAR(ACE_FEC_FIN,'DD/MM/YYYY HH24:MI:SS') FROM ACCALEVENTOS WHERE ACE_ANIO=APE_ANO AND ACE_PERIODO=APE_PER AND ace_cod_evento = 72 AND ace_cra_cod=cra_cod) FIN_CAMBIO_ESTADOS,
(SELECT TO_CHAR(ACE_FEC_INI,'DD/MM/YYYY HH24:MI:SS') FROM ACCALEVENTOS WHERE ACE_ANIO=APE_ANO AND ACE_PERIODO=APE_PER AND ace_cod_evento = 73 AND ace_cra_cod=cra_cod) INICIO_APLICAR_REGLAMENTO,
(SELECT TO_CHAR(ACE_FEC_FIN,'DD/MM/YYYY HH24:MI:SS') FROM ACCALEVENTOS WHERE ACE_ANIO=APE_ANO AND ACE_PERIODO=APE_PER AND ace_cod_evento = 73 AND ace_cra_cod=cra_cod) FIN_APLICAR_REGLAMENTO,
tra_nivel
FROM ACCALEVENTOS
INNER JOIN accra ON cra_cod=ace_cra_cod
INNER JOIN actipcra ON tra_cod=cra_tip_cra
INNER JOIN gedep ON dep_cod=cra_dep_cod
INNER JOIN acasperipreinsdemanda ON ape_ano=ace_anio AND ape_per=ace_periodo
WHERE ace_cod_evento=60
AND ace_cra_cod=cra_cod
AND ape_estado='P'
ORDER BY tra_nivel desc,dep_nombre,cra_cod</SQLRaw></SQL><QueryColumns><QueryColumn><Name>CRA_COD</Name><TableName></TableName><ColumnName>CRA_COD</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML>2%</ColumnWidthHTML><column_title>COD.</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>CRA_NOMBRE</Name><TableName></TableName><ColumnName>CRA_NOMBRE</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF>120 mm</ColumnWidthPDF><ColumnWidthHTML>26%</ColumnWidthHTML><column_title>PROYECTO CURRICULAR</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>INICIO_CIERRE</Name><TableName></TableName><ColumnName>(SELECT TO_CHAR(ACE_FEC_INI,'DD/MM/YYYY HH24:MI:SS') FROM ACCALEVENTOS WHERE ACE_ANIO=APE_ANO AND ACE_PERIODO=APE_PER AND ace_cod_evento = 60 AND ace_cra_cod=cra_cod)</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML>9%</ColumnWidthHTML><column_title>INICIO CIERRE</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><gro