<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Listado matrículados a primer semestre</ReportTitle><ReportDescription>Muestra el listado de los estudiantes que fueron admitidos y se matricularon.</ReportDescription><PageSize>US-Letter</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>8</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType></DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT row_number() over () as rownum,PERIODO,COD_INICIAL,COD_CRA,CRA_NOM,PENSUM,DOCU_COORDINADOR,CODIGO,NOMBRE,NRO_IDEN,ACUERDO,CREDITOS,EMAIL,ESTADO,ESTADO_ACTUAL
FROM (
SELECT DISTINCT
substr(CAST(A.est_cod AS TEXT),1,4)||'-'||substr(CAST(A.est_cod AS TEXT),5,1) AS PERIODO,
substr(CAST(A.est_cod AS TEXT),1,5) 	AS COD_INICIAL,
A.est_cra_cod 		AS COD_CRA,
CRA.cra_nombre 		AS CRA_NOM,
A.est_pen_nro   		AS PENSUM,
CRA.cra_emp_nro_iden	AS DOCU_COORDINADOR,
A.est_cod  		AS CODIGO,
A.est_nombre  		AS NOMBRE,
A.est_nro_iden 		AS NRO_IDEN,
case when A.est_acuerdo=1993027 then '027 de 1993'
when A.est_acuerdo=2009007 then '007 de 2009'
when A.est_acuerdo=2011004 then '004 de 2011'
when A.est_acuerdo is null then 'NO REGISTRA' end ACUERDO,
case when A.est_ind_cred='S' then 'CREDITOS'
when A.est_ind_cred='N' then 'HORAS'
else '' end AS CREDITOS,
eot_email		AS EMAIL,
est_estado_est ESTADO,
estado_descripcion ESTADO_ACTUAL
FROM
acest A
inner join accra CRA on A.est_cra_cod=CRA.cra_cod
left outer join acestotr on A.est_cod=eot_cod
inner join acestado on A.est_estado_est=estado_cod
WHERE A.est_estado_est!='N'
ORDER BY
PERIODO,A.est_cra_cod,A.est_cod) as matriculados</SQLRaw></SQL><QueryColumns><QueryColumn><Name>rownum</Name><TableName></TableName><ColumnName>row_number() over () as</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>No.</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>PERIODO</Name><TableName></TableName><ColumnName>PERIODO</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF>20mm</ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>PERÍODO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>COD_INICIAL</Name><TableName></TableName><ColumnName>COD_INICIAL</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>COD_CRA</Name><TableName></TableName><ColumnNam