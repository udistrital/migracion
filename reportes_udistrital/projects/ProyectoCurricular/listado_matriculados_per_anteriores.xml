<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Listado matrículados períodos anteriores</ReportTitle><ReportDescription>Muestra el listado de estudiantes matrículados en los períodos anteriores al actual</ReportDescription><PageSize>US-Letter</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>8</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType></DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT 
	(B.est_ano||'-'||B.est_per) 	AS PERIODO,
	dep_cod				AS COD_FACULTAD,
	dep_nombre			AS FACULTAD,
	B.est_cra_cod 			AS COD_PROYECTO,
	cra_nombre			AS PROYECTO,
	B.est_cod 			AS CODIGO,
	A.est_nombre			AS NOMBRE,
	A.est_nro_iden 			AS NRO_IDEN,
	A.est_pen_nro 			AS PENSUM,
case when A.est_acuerdo=1993027 then '027 de 1993'
when A.est_acuerdo=2009007 then '007 de 2009'
when A.est_acuerdo=2011004 then '004 de 2011'
when A.est_acuerdo is null then 'NO REGISTRA' end 	AS ACUERDO,
case when A.est_ind_cred='S' then 'CREDITOS'
when A.est_ind_cred='N' then 'HORAS'
else '' end AS CREDITOS,
	B.est_estado			AS COD_ESTADO,
	ANTERIOR.estado_descripcion 		AS ESTADO,
	A.est_estado_est			AS COD_ESTADO_ACTUAL,
	ACTUAL.estado_descripcion 		AS ESTADO_ACTUAL
FROM acesthis B
INNER JOIN accra CRA ON cra_cod=B.est_cra_cod
INNER JOIN actipcra on cra_tip_cra=tra_cod
INNER JOIN gedep on dep_cod=cra_dep_cod
INNER JOIN acestado ANTERIOR ON B.est_estado=ANTERIOR.estado_cod
INNER JOIN acest A ON A.est_cod=B.est_cod
LEFT OUTER JOIN acestado ACTUAL ON A.est_estado_est=ACTUAL.estado_cod
WHERE B.EST_ESTADO IN ('A','B','H','L')
	AND cra_estado='A'
	AND tra_nivel='PREGRADO'
ORDER BY dep_cod, B.est_cra_cod, B.est_cod
</SQLRaw></SQL><QueryColumns><QueryColumn><Name>PERIODO</Name><TableName></TableName><ColumnName>(B.est_ano||'-'||B.est_per) 	AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>PERÍODO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>COD_FACULTAD</Name><TableName></TableName><ColumnName>dep_cod				AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>FACULTAD</Name><TableName></TableName><ColumnName>dep_nombre			AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>FACULTAD</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>COD_PROYECTO</Name><TableName></TableName><ColumnName>B.est_cra_cod 			AS