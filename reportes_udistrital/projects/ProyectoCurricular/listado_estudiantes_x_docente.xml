ACEST.EST_COD CODIGO,
ACEST.EST_NOMBRE NOMBRE, 
ACEST.EST_TELEFONO TELEFONO,  
ACESTOTR.EOT_EMAIL EMAIL, 
DOC_NRO_IDEN IDENTIFICACION, 
DOC_APELLIDO||' '||DOC_NOMBRE DOCENTE,
ACINSHIS.INS_ANO||'-'||ACINSHIS.INS_PER PERIODO,
asi_cod COD_ASIGNATURA,
asi_nombre ASIGNATURA 
FROM ACCARGAHIS 
INNER JOIN ACDOCENTE ON CAR_DOC_NRO_IDEN = DOC_NRO_IDEN
INNER JOIN ACINSHIS ON INS_ASI_COD = CAR_CUR_ASI_COD AND INS_GR=CAR_CUR_NRO AND INS_ANO=CAR_APE_ANO AND INS_PER = CAR_APE_PER AND INS_CRA_COD=CAR_CRA_COD
INNER JOIN ACASI ON CAR_CUR_ASI_COD=asi_cod
INNER JOIN ACEST ON EST_COD = INS_EST_COD
INNER JOIN ACESTOTR ON EST_COD = EOT_COD
ORDER BY ACINSHIS.INS_ANO||'-'||ACINSHIS.INS_PER desc, asi_cod,ACEST. EST_COD</SQLRaw></SQL><QueryColumns><QueryColumn><Name>CODIGO</Name><TableName></TableName><ColumnName>DISTINCT 
