<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Cursos por espacio academico</ReportTitle><ReportDescription>Presenta el listado de los cursos creados por espacio académico, indicando el horario y los docentes con carga en el curso</ReportDescription><PageSize>US-Letter</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType></DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>select distinct cur_ape_ano,
cur_ape_per,
cur_asi_cod||' - '||asi_nombre espacio,
cur_grupo,
cur_grupo||' ('||cur_id||')' grupo,
cur_cra_cod,
cur_cra_cod||' - '||A.cra_nombre proyecto,
H.hor_dia_nro,
dia_abrev,
H.hor_hora,
GH.hor_larga,
car_doc_nro,
car_doc_nro||' - '||doc_nombre||' '||doc_apellido NOMBRE,
car_tip_vin,
tvi_nombre
from accursos
inner join acasperi on ape_ano=cur_ape_ano and ape_per=cur_ape_per
left outer join achorarios H ON hor_id_curso=cur_id
left outer join accargas on car_hor_id=hor_id and car_estado='A'
left outer join accra A ON A.cra_cod=cur_cra_cod
left outer join acasi ON asi_cod=cur_asi_cod
left outer join acdocente ON doc_nro_iden=car_doc_nro
left outer join gehora GH on H.hor_hora=GH.hor_cod
left outer join gedia on hor_dia_nro=dia_cod
left outer join actipvin ON tvi_cod=car_tip_vin
where ape_estado='A'
and cur_estado='A'
and H.hor_estado='A'
order by cur_cra_cod,espacio,cur_grupo,car_doc_nro,hor_dia_nro,hor_hora</SQLRaw></SQL><QueryColumns><QueryColumn><Name>cur_ape_ano</Name><TableName></TableName><ColumnName>distinct</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>AÑO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>cur_ape_per</Name><TableName></TableName><ColumnName>cur_ape_per</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>PERIODO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>espacio</Name><TableName></TableName><ColumnName>cur_asi_cod||' - '||asi_nombre</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>ESPACIO ACADEMICO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>