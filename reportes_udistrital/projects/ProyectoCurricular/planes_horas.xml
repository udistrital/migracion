<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Planes de estudio de horas</ReportTitle><ReportDescription>Presenta la información de los espacios académicos de los planes de estudios de horas del Proyecto Curricular.</ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType></DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT pen_cra_cod COD_PROYECTO,
cra_nombre PROYECTO_CURRICULAR,
pen_cra_cod||' - '||cra_nombre PROYECTO,
pen_nro COD_PLAN,
pen_asi_cod COD_ESPACIO,
asi_nombre NOMBRE_ESPACIO,
CASE pen_estado
   WHEN 'A' THEN 'ACTIVO'
   WHEN 'I' THEN 'INACTIVO'
   ELSE ''
END ESTADO,
pen_sem NIVEL,
CASE pen_ind_ele
   WHEN 'N' THEN 'OBLIGATORIO'
   WHEN 'S' THEN 'ELECTIVO'
   ELSE pen_ind_ele
END ELECTIVA,
pen_cre CREDITOS,
pen_nro_ht HT,
pen_nro_hp HP
FROM acpen
INNER JOIN acasi ON asi_cod=pen_asi_cod
INNER JOIN accra ON cra_cod=pen_cra_cod
INNER JOIN acplanestudio ON plan_cra_cod=pen_cra_cod AND plan_pen_nro=pen_nro
WHERE plan_tipo=1
ORDER BY pen_cra_cod,pen_nro,pen_sem,pen_asi_cod</SQLRaw></SQL><QueryColumns><QueryColumn><Name>COD_PROYECTO</Name><TableName></TableName><ColumnName>pen_cra_cod</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>COD. PROYECTO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>PROYECTO_CURRICULAR</Name><TableName></TableName><ColumnName>cra_nombre</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>PROYECTO CURRICULAR</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>PROYECTO</Name><TableName></TableName><ColumnName>pen_cra_cod||' - '||cra_nombre</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>PROYECTO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Nam