<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Listado estudiantes por grupo para firma</ReportTitle><ReportDescription>Presenta el listado de estudiantes por grupo como documento para firma.</ReportDescription><PageSize>US-Letter</PageSize><PageOrientation>Portrait</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>9</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType></DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT distinct to_char(current_timestamp, 'dd-mon-yyyy hh24:mi') FECHA,
ins_asi_cod COD_ESPACIO,
asi_cod||' - '||asi_nombre ESPACIO,
(Lpad(Cur_Cra_Cod::text,3,'0')||'-'||Cur_Grupo) GRUPO,
cur_grupo COD_GRUPO,
A.est_cod COD_ESTUDIANTE,
A.est_nombre NOMBRE_ESTUDIANTE,
A.est_nro_iden DOCUMENTO,
A.est_estado_est ESTADO,
'&lt;br&gt;__________________________' FIRMA
FROM acest A
INNER JOIN acins on ins_est_cod=est_cod
INNER JOIN accursos ON cur_id=ins_gr
INNER JOIN acasi ON asi_cod=ins_asi_cod
INNER JOIN acasperi ON ape_ano=cur_ape_ano AND ape_per=cur_ape_per
WHERE ape_estado='A'
ORDER BY ins_asi_cod,(Lpad(Cur_Cra_Cod::text,3,'0')||'-'||Cur_Grupo),A.est_cod</SQLRaw></SQL><QueryColumns><QueryColumn><Name>FECHA</Name><TableName></TableName><ColumnName>distinct to_char(current_timestamp, 'dd-mon-yyyy hh24:mi')</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>FECHA</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>COD_ESPACIO</Name><TableName></TableName><ColumnName>ins_asi_cod</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>COD. ESPACIO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>ESPACIO</Name><TableName></TableName><ColumnName>asi_cod||' - '||asi_nombre</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>ESPACIO ACADEMICO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_tr