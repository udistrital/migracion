<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Listado matriculados período actual</ReportTitle><ReportDescription>Muestra el listado de estudiantes matrículados en el período actual</ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>.DEFAULT</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>8</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType></DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT 
	(ape_ano||'-'||ape_per) 	AS PERIODO,
	dep_cod				AS COD_FACULTAD,
	dep_nombre			AS FACULTAD,
	est_cra_cod 			AS COD_PROYECTO,
	cra_nombre			AS PROYECTO,
	est_cod 			AS CODIGO,
	est_nombre			AS NOMBRE,
	est_nro_iden 			AS NRO_IDEN,
	est_pen_nro 			AS PENSUM,
	decode(B.est_acuerdo,1993027,'027 de 1993',2009007,'007 de 2009',2011004,'004 de 2011',B.est_acuerdo) 	AS ACUERDO,
	DECODE(est_ind_cred,'S', 'CREDITOS', 'N', 'HORAS') AS CREDITOS,
	est_estado_est			AS COD_ESTADO,
	estado_descripcion 		AS ESTADO_ACTUAL,
        eot_email                       AS MAIL,
        eot_email_ins                   AS MAIL_INS
FROM acest B
INNER JOIN accra CRA ON cra_cod=est_cra_cod
INNER JOIN actipcra on cra_tip_cra=tra_cod
INNER JOIN gedep on dep_cod=cra_dep_cod
INNER JOIN acasperi on ape_estado='A'
INNER JOIN acestado on est_estado_est=estado_cod
INNER JOIN ACESTOTR ON est_cod=eot_cod
WHERE EST_ESTADO_EST IN ('A','B','H','L','C')
	AND cra_estado='A'
	AND tra_nivel='PREGRADO'
ORDER BY dep_cod, est_cra_cod, est_cod</SQLRaw></SQL><QueryColumns><QueryColumn><Name>PERIODO</Name><TableName></TableName><ColumnName>(ape_ano||'-'||ape_per) 	AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>PERÍODO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>COD_FACULTAD</Name><TableName></TableName><ColumnName>dep_cod				AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>FACULTAD</Name><TableName></TableName><ColumnName>dep_nombre			AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>FACULTAD</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>COD_PROYECTO</Name><TableName></TableName