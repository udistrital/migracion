<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Espacios reprobados no inscritos</ReportTitle><ReportDescription>Permite conocer el listado de los espacios perdidos por cada estudiante activo que no se han inscrito en el período actual.</ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>.DEFAULT</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType></DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>select A.est_cra_cod COD_PROYECTO,
cra_nombre PROYECTO_CURRICULAR,
A.est_cod COD_ESTUDIANTE,
A.est_nombre NOMBRE_ESTUDIANTE,
A.est_estado_est ESTADO,
E.estado_descripcion DESCRIPCION,
not_asi_cod ,
asi_nombre,
lpad(not_nota::TEXT,2,'0') NOTA,
CASE WHEN nob_nombre='SIN OBSERVACION' THEN ''
ELSE nob_nombre END AS OBSERVACION
from acest A
inner join accra on cra_cod=A.est_cra_cod
inner join gedep on dep_cod=cra_dep_cod
inner join acestado E on estado_cod=est_estado_est
inner join acnot on not_est_cod=A.est_cod and not_cra_cod=A.est_cra_cod
inner join acnotobs on not_obs=nob_cod
inner join acasi on asi_cod=not_asi_cod
inner join acasperi on ape_ano=not_ano and ape_per=not_per
where ((not_nota&lt;30 and not_obs not in (19,22,24)) OR not_obs in (20,23,25))
and ape_estado='P'
and est_estado_est in ('A','B','H')
and not_asi_cod not in (select ins_asi_cod from acins
inner join acasperi on ape_ano=ins_ano and ape_per=ins_per
where ins_est_cod=A.est_cod and ape_estado='A')
order by cra_dep_cod,A.est_cra_cod,est_cod</SQLRaw></SQL><QueryColumns><QueryColumn><Name>COD_PROYECTO</Name><TableName>A</TableName><ColumnName>est_cra_cod</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>CÓD. PROYECTO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>PROYECTO_CURRICULAR</Name><TableName></TableName><ColumnName>cra_nombre</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>PROYECTO CURRICULAR</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>COD_ESTUDIANTE</Name><TableName>A</TableName><ColumnName>est_cod</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>COD. ESTUDIANTE</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>NOMBRE_ESTUD