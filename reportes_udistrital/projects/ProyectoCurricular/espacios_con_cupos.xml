<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Espacios con cupos disponibles</ReportTitle><ReportDescription>Presenta el listado de espacios con cursos registrados que tienen cupos disponibles</ReportDescription><PageSize>US-Letter</PageSize><PageOrientation>Portrait</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType></DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>select distinct cur_ape_ano,
cur_ape_per,
cra_nombre,
cur_asi_cod,
asi_nombre,
(Lpad(Cur_Cra_Cod::text,3,'0')||'-'||Cur_Grupo) grupo,
cur_nro_cupo,
cur_nro_cupo-cur_nro_ins disponibles
from accursos
inner join acasperi on ape_ano=cur_ape_ano and ape_per=cur_ape_per
left outer join achorarios ON hor_id_curso=cur_id
left outer join accra on cra_cod=cur_cra_cod
inner join acasi on asi_cod=cur_asi_cod
inner join actipcra ON tra_cod=cra_tip_cra
where ape_estado='A'
and cur_nro_cupo&gt;cur_nro_ins
and cur_estado='A'
AND (tra_nivel='PREGRADO' or tra_nivel='EXTENSION')
order by cra_nombre,cur_asi_cod</SQLRaw></SQL><QueryColumns><QueryColumn><Name>cur_ape_ano</Name><TableName></TableName><ColumnName>distinct</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>ANO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>cur_ape_per</Name><TableName></TableName><ColumnName>cur_ape_per</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>PERIODO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>cur_asi_cod</Name><TableName></TableName><ColumnName>cur_asi_cod</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>COD.</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>asi_nombre</Name><TableName></TableName><ColumnName>asi_nombre</Colu