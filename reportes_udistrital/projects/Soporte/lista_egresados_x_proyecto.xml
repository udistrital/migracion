<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Lista de Egresados por Proyecto</ReportTitle><ReportDescription>Este reporte presenta la información de los egresados de un proyecto curricular por año de grado.</ReportDescription><PageSize>US-Letter</PageSize><PageOrientation>Portrait</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType></DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT
DISTINCT(EST_COD) EST_COD,
EST_NOMBRE,
EST_NRO_IDEN,
EGR_CRA_COD,
CRA_NOMBRE,
EST_SEXO,
TIT_NOMBRE,
EGR_TRABAJO_GRADO,
EGR_DIRECTOR_TRABAJO,
EGR_FECHA_GRADO,
EGR_ACTA_GRADO,
EGR_LIBRO,
EGR_FOLIO,
EGR_REG_DIPLOMA,
EGR_DIRECCION_CASA,
EGR_EMAIL,
EGR_TELEFONO_CASA,
CONCAT(CONCAT(REC_NOMBRE, ' '),REC_APELLIDO) REC_NOM,
CONCAT(CONCAT(SEC_NOMBRE, ' '),SEC_APELLIDO) SEC_NOM,
to_char(EGR_FECHA_GRADO,'yyyy') ANIO_GRADO
FROM ACEST
LEFT OUTER JOIN ACEGRESADO ON EST_COD = EGR_EST_COD AND  EST_CRA_COD= EGR_CRA_COD
LEFT OUTER JOIN ACCRA ON EST_CRA_COD = CRA_COD
LEFT OUTER JOIN ACTITULO ON EGR_TITULO=TIT_COD AND EGR_CRA_COD=TIT_CRA_COD AND EGR_SEXO=TIT_SEXO
LEFT OUTER JOIN ACRECTOR ON EGR_RECTOR=REC_COD
LEFT OUTER JOIN ACSECRETARIO ON EGR_SECRETARIO=SEC_COD
WHERE 
trim(EST_ESTADO_EST) ='E'
ORDER BY EGR_FECHA_GRADO desc,EST_COD,EGR_CRA_COD</SQLRaw></SQL><QueryColumns><QueryColumn><Name>EST_COD</Name><TableName></TableName><ColumnName>DISTINCT(EST_COD)</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>CÓD. ESTUDIANTE</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>CRA_NOMBRE</Name><TableName></TableName><ColumnName>CRA_NOMBRE</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>CARRERA</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>TIT_NOMBRE</Name><TableName></TableName><ColumnName>TIT_NOMBRE</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>TÍTULO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>EGR_TRABAJO_GRADO</Name><TableName>