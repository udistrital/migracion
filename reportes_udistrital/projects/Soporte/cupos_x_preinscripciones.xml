<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Cupos preinscripción automática</ReportTitle><ReportDescription>Presenta los cursos cuyo número de inscritos es superior al número de cupos establecido por el Proyecto.</ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>.DEFAULT</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType></DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>select cur_ape_ano ANO,
cur_ape_per PERIODO,
cur_cra_cod COD_PROYECTO,
cur_cra_cod||'-'||cra_nombre PROYECTO,
cur_asi_cod COD_ESPACIO,
asi_nombre ESPACIO,
cur_id ID_GRUPO,
(LPAD(CAST(Cur_Cra_Cod AS TEXT),3,'0')||'-'||Cur_Grupo) GRUPO,
cur_nro_cupo CUPO,
(select count(ins_est_cod) from acinspre_his
where ins_asi_cod=cur_asi_cod
and ins_ano=cur_ape_ano
and ins_per=cur_ape_per
and ins_gr=cur_id) INSCRITOS,
(cur_nro_cupo-(select count(ins_est_cod) from acinspre_his
where ins_asi_cod=cur_asi_cod
and ins_ano=cur_ape_ano
and ins_per=cur_ape_per
and ins_gr=cur_id)) DIFERENCIA
from accursos
inner join acasi on asi_cod=cur_asi_cod
inner join accra on cra_cod=cur_cra_cod
inner join acasperi on ape_ano=cur_ape_ano and ape_per=cur_ape_per
where ape_estado='A'
and cur_nro_cupo&lt;(select count(ins_est_cod) from acinspre_his
where ins_asi_cod=cur_asi_cod
and ins_ano=cur_ape_ano
and ins_per=cur_ape_per
and ins_gr=cur_id)
--and cur_nro_cupo&gt;0
--and cra_dep_cod=32
order by cur_cra_cod,cur_Asi_cod</SQLRaw></SQL><QueryColumns><QueryColumn><Name>ANO</Name><TableName></TableName><ColumnName>cur_ape_ano</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>AÑO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>PERIODO</Name><TableName></TableName><ColumnName>cur_ape_per</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>PERIODO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>COD_PROYECTO</Name><TableName></TableName><ColumnName>cur_cra_cod</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>COD. PROYECTO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>PROYECTO</Name><TableName></TableName><ColumnName>cur_cra_cod||'-'