<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Recibos ECAES pagados</ReportTitle><ReportDescription>Muestra el listado de los recibos pagados por concepto de ECAES en un rango de tiempo determinado.</ReportDescription><PageSize>US-Letter</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>8</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType></DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT DISTINCT DEP_COD COD_FAC,
   DEP_NOMBRE FACULTAD,
   CRA_COD COD_CRA,
   CRA_NOMBRE CARRERA,
   EST_COD CODIGO,
   (SUBSTR(EAN_APELLIDOS,1,INSTR(EAN_APELLIDOS,' ',1,1)-1)||'|'||
   SUBSTR(EAN_APELLIDOS,INSTR(EAN_APELLIDOS,' ',1,1)+1)||'|'||
   SUBSTR(EAN_NOMBRES,1,INSTR(EAN_NOMBRES,' ',1,1)-1)||'|'||
   SUBSTR(EAN_NOMBRES,INSTR(EAN_NOMBRES,' ',1,1)+1)||'|'||
   'C'||'|'||
   EST_NRO_IDEN) REGISTRO,	
   FUA_TIPO_PAGO_ECAES(RBA_VALOR) TIPO_PAGO,
   EMA_FECHA,
   rba_valor VALOR_PAGADO
FROM ACASPERI, ACVLRSCS, GEDEP, ACCRA, ACESTMAT a, ACRECBAN, ACEST, ACREFEST, V_EST_APELLIDOS_NOMBRES
WHERE APE_ANO = EMA_ANO
   AND APE_PER = EMA_PER
   AND EMA_ANO = VLR_ANO
   AND EMA_PER = VLR_PER
   AND DEP_COD = CRA_DEP_COD
   AND CRA_COD = EMA_CRA_COD
   AND APE_ANO = EMA_ANO
   AND EST_COD = EMA_EST_COD
   AND AER_ANO = EMA_ANO
   AND AER_SECUENCIA = EMA_SECUENCIA
   AND EMA_SECUENCIA = RBA_SECUENCIA
   AND EMA_EST_COD = RBA_COD
   AND EST_COD = EAN_EST_COD
   AND EMA_ANO::TEXT||EMA_SECUENCIA::TEXT IN (SELECT CAST(AER_ANO AS TEXT)||CAST(AER_SECUENCIA AS TEXT)
                                  FROM ACREFEST
     WHERE a.EMA_ANO = AER_ANO
    AND a.EMA_SECUENCIA = AER_SECUENCIA
    AND AER_REFCOD = 14)

   AND CAST((RBA_ANO)||LPAD(CAST(RBA_MES AS TEXT),2,'0')||LPAD(CAST(RBA_DIA AS TEXT),2,'0') AS INT) 
         BETWEEN (SELECT MIN(CAST(TO_CHAR(EMA_FECHA,'yyyymmdd') AS INT))
                       FROM ACESTMAT
   	  WHERE a.EMA_ANO = EMA_ANO
     AND a.EMA_PER = EMA_PER)
              AND     (SELECT MAX(CAST(TO_CHAR(EMA_FECHA_EXT,'yyyymmdd') AS INT))
                       FROM ACESTMAT
  WHERE a.EMA_ANO = EMA_ANO
     AND a.EMA_PER = EMA_PER)     
    
ORDER BY 1,3,5 ASC</SQLRaw></SQL><QueryColumns><QueryColumn><Name>COD_FAC</Name><TableName></TableName><ColumnName>DISTINCT DEP_COD</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>FACULTAD</Name><TableName></TableName><ColumnName>DEP_NOMBRE</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>FACULTAD</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>COD_CRA</Name><TableName></TableName><ColumnName>CRA_COD</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF>15mm</ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>COD. PROYECTO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>CARRERA</Name><TableName></TableName><ColumnName>CRA_NOMBRE</ColumnName><C