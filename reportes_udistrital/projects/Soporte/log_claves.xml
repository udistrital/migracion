<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>log de claves</ReportTitle><ReportDescription>Muestra el listado del log de las claves de un usuario en un intervalo de fechas</ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>.DEFAULT</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>8</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType></DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT  
	cla_codigo  	AS CODIGO,
	cla_tipo_usu  	AS COD_TIPO,
	usutipo_tipo  	AS TIPO_USUARIO,
	cla_estado  	AS ESTADO,
	to_char(cla_fecha, 'yyyy-mm-dd') 	AS FECHA,
	cla_usuario 	AS USUARIO,
	cla_terminal	AS TERMINAL,
	cla_op		AS COD_OPERACION,
	DECODE (TRIM(cla_op),'M','MODIFICA','C','REGISTRA','B','BORRA',cla_op) OPERACION
FROM geclaveslog
INNER JOIN geusutipo ON cla_tipo_usu=usutipo_cod
WHERE (cla_terminal NOT LIKE '%OAS%' OR cla_terminal IS NULL)
ORDER BY FECHA DESC</SQLRaw></SQL><QueryColumns><QueryColumn><Name>CODIGO</Name><TableName></TableName><ColumnName>cla_codigo  	AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>CÓDIGO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>COD_TIPO</Name><TableName></TableName><ColumnName>cla_tipo_usu  	AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF>15mm</ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>COD. TIPO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>TIPO_USUARIO</Name><TableName></TableName><ColumnName>usutipo_tipo  	AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF>40mm</ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>TIPO USUARIO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>ESTADO</Name><TableName></TableName><ColumnNa