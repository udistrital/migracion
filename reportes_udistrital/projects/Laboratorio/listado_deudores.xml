<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Listado deudores</ReportTitle><ReportDescription>Muestra el listado de deudores por facultad, agrupados por proyecto curricular</ReportDescription><PageSize>US-Letter</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>8</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType></DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT 
dep_cod 	AS COD_FACULTAD,
dep_nombre  	AS FACULTAD,
cra_cod  	AS COD_PROYECTO,
cra_nombre 	AS PROYECTO,
deu_est_cod   	AS COD_ESTUDIANTE, 
est_nombre   	AS NOMBRE,
deu_cpto_cod  	AS COD_CONCEPTO, 
deu_material  	AS MATERIAL, 
deu_ano       	AS ANIO, 
deu_per       	AS PERIODO, 
deu_estado    	AS ESTADO, 
cpto_nombre   	AS CONCEPTO,
TO_CHAR(deu_fecha,'YYYY-mm-dd') 	AS FECHA,
(CASE WHEN deu_multa IS NOT NULL AND deu_multa &gt;0 THEN cast(deu_multa as text)  ELSE '' END) 	AS VALOR
FROM acdeudores
INNER JOIN acconcepto ON  cpto_cod = deu_cpto_cod
LEFT OUTER JOIN acest ON  deu_est_cod = est_cod
LEFT OUTER JOIN accra ON  est_cra_cod = cra_cod
LEFT OUTER JOIN gedep ON  cra_dep_cod = dep_cod
ORDER BY dep_cod, cra_cod, deu_est_cod,deu_fecha</SQLRaw></SQL><QueryColumns><QueryColumn><Name>COD_FACULTAD</Name><TableName></TableName><ColumnName>dep_cod 	AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>COD. FACULTAD</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>FACULTAD</Name><TableName></TableName><ColumnName>dep_nombre  	AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>FACULTAD</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>COD_PROYECTO</Name><TableName></TableName><ColumnName>cra_cod  	AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>COD. PROYECTO</column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>PROYECTO</Name><TableNam