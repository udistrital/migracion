	<queryString>
		<![CDATA[SELECT unique CIR_EMP_NRO_IDEN,
   CIR_ANO,
   CIR_DESDE,
   to_char(CIR_DESDE,'yyyy') DESDEA,
   to_char(CIR_DESDE,'mm') DESDEM,
   to_char(CIR_DESDE,'dd') DESDED,
   CIR_HASTA,
   to_char(CIR_HASTA,'yyyy') HASTAA,
   to_char(CIR_HASTA,'mm') HASTAM,
   to_char(CIR_HASTA,'dd') HASTAD,
   CIR_SALARIO,
   CIR_CESANTIA,
   CIR_GASTOS_REPRESENTACION,
   CIR_PENSION,
   CIR_OTROS,
   CIR_TOTAL_INGRESOS,
   CIR_APORTES_SALUD,
   CIR_APORTE_VOLUNTARIO,
   nvl(CIR_APORTES_PENSION,0),
   CIR_EXENTAS,
   CIR_RETENCION,
   CIR_ESTADO,
   EMP_NOMBRE,
   SYSDATE,
   to_char(sysdate,'yyyy') FECHAA,
   to_char(sysdate,'mm') FECHAM,
   to_char(sysdate,'dd') FECHAD,
------------------------------------------------------------------------------
   (select EMP_NOMBRE
    from MNTPE.PEEMP, MNTGE.GELUGAR
    where EMP_CAR_COD = 5
       and EMP_ESTADO_E = 'A'
       and EMP_LUG_COD_IDEN = LUG_COD) NOMBRETESORERO,
   (select EMP_NRO_IDEN
    from MNTPE.PEEMP, MNTGE.GELUGAR
    where EMP_CAR_COD = 5
       and EMP_ESTADO_E = 'A'
       and EMP_LUG_COD_IDEN = LUG_COD) CEDULATESORERO,
   (select LUG_NOMBRE
    from MNTPE.PEEMP, MNTGE.GELUGAR
    where EMP_CAR_COD = 5
       and EMP_ESTADO_E = 'A'
       and EMP_LUG_COD_IDEN = LUG_COD) LUGARTESORERO,
------------------------------------------------------------------------------
  (SELECT FUA_REDONDEO_MIL(4500 * UVT_VALOR)
   FROM PEVALORUVT
   WHERE UVT_ANO = $P{anio}) UVT45,
  (SELECT FUA_REDONDEO_MIL(4073 * UVT_VALOR)
   FROM PEVALORUVT
   WHERE UVT_ANO = $P{anio}) UVT33,
  (SELECT FUA_REDONDEO_MIL(2800 * UVT_VALOR)
   FROM PEVALORUVT
   WHERE UVT_ANO = $P{anio}) UVT28
------------------------------------------------------------------------------
FROM PRCERINGRET2004, PEEMP, DUAL
WHERE CIR_EMP_NRO_IDEN = EMP_NRO_IDEN
   and CIR_ANO = $P{anio}
   and EMP_NRO_IDEN = $P{usuario}
   and EMP_ESTADO = 'A'
   and CIR_ESTADO = 'A'
ORDER BY EMP_NOMBRE ASC]]>
	</queryString>
